increment_percent=0
def find_new_salary(current_salary,job_level):
    if(job_level==3):   
        increment_percent=((current_salary*15)/100)
    elif(job_level==4):
        increment_percent=((current_salary*7)/100)
    elif(job_level==5):
         increment_percent=((current_salary*4)/100)
    else:
        print(current_salary)
    new_salary=current_salary+increment_percent    
    return new_salary
new_salary=find_new_salary(10000,3)
print(new_salary)
